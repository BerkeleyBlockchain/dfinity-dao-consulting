{
  "canisters": {
    "rust_starter": {
      "build": "cargo build --target wasm32-unknown-unknown --package rust_starter --release",
      "candid": "src/rust_starter/src/rust_starter.did",
      "wasm": "target/wasm32-unknown-unknown/release/rust_starter.wasm",
      "type": "custom",
      "main": "src/rust_starter/src/lib.rs"
    },
    "rust_starter_assets": {
      "dependencies": [
        "rust_starter"
      ],
      "frontend": {
        "entrypoint": "src/rust_starter_assets/src/index.html"
      },
      "source": [
        "src/rust_starter_assets/assets",
        "dist/rust_starter_assets/"
      ],
      "type": "assets"
    },
    "tokens": {
      "build": "cargo build --target wasm32-unknown-unknown --package tokens --release",
      "candid": "src/tokens/src/token.did",
      "wasm": "target/wasm32-unknown-unknown/release/tokens.wasm",
      "type": "custom",
      "main": "src/tokens/src/main.rs"
    },
    "minting": {
      "build": "cargo build --target wasm32-unknown-unknown --package minting --release",
      "candid": "src/minting/src/minting.did",
      "wasm": "target/wasm32-unknown-unknown/release/minting.wasm",
      "type": "custom",
      "main": "src/minting/src/main.rs"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "dfx": "0.8.4",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
